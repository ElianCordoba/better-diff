{
  "name": "better-diff",
  "description": "Aiming to be the next-gen `git diff`",
  "version": "0.0.1",
  "license": "MIT",
  "repository": "ElianCordoba/better-diff",
  "author": {
    "name": "Elian Cordoba"
  },
  "scripts": {
    "test": "vitest --dir ./tests",
    "build": "tsc --watch",
    "check": "npm run format && npm run lint",
    "format": "deno fmt --line-width 300 --ignore=node_modules,diff,internal,dist,tests,ui,scripts",
    "lint": "deno lint --ignore=node_modules,diff,internal,dist,tests,ui,scripts",
    "startServer": "tsx src/server.ts",
    "start": "node dist/src/server.js",
    "ui": "npm run  dev --prefix ./ui",
    "bench": "node ./dist/bench/bench.js",
    "debug": "node --inspect-brk dist/src/server.js",
    "profile": "pprof-it ./dist/scripts/test.js",
    "view": "pprof -http=:"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.2",
    "benchmark": "^2.1.4",
    "pprof-it": "^1.2.0",
    "vite": "4.0.3",
    "vitest": "0.29.8"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "fastify": "^4.10.2",
    "kleur": "^4.1.5",
    "typescript": "5.0.3"
  }
}
